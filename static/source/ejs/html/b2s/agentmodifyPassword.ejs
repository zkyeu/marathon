<% include ../common/top.ejs %>
<body id="container" data-init="agentfindPassword">
<!-- 公共头 -->
<% include ../common/b2s/b2s_header.ejs %>
<!-- 中间内容 -->
<div class="ce-content agent-findpsw">
	<div class="widthMain">
		<div class="perfect-info">
			<h3 class="info-tit"><span class="info-suc">修改密码</span></h3>
			<form action="http://www.baidu.com" method="get" id="find_password_Form" rel="forgetpsw">
				<ul class="info-cont">
					<li>
						<span class="info-user">手机号：</span><input placeholder="请输入手机号" type="text" class="china-name findjsBlank mobile-text tel readonly-input" rel="phone" maxlength="11" value="13376023558" readonly="readonly" /><span class="get-code-btn sendCode" send-url="http://www.baidu.com" send-type="4">获取验证码</span>
						<p class="info-tip hide find-error"><span class="tenIcon err-icon"></span>请输入手机号</p>
					</li>
					<li>
						<span class="info-user">验证码：</span><input placeholder="请输入验证码" type="text" class="china-name findjsBlank" rel="verifycode" maxlength="6" />
						<p class="info-tip hide"><span class="tenIcon err-icon"></span>请输入验证码</p>
					</li>
					<li>
						<span class="info-user">新密码：</span><input placeholder="请输入密码"  type="password" class="china-name findjsBlank" rel="password" />
						<p class="info-tip hide"><span class="tenIcon err-icon"></span>请输入密码</p>
					</li>
					<li>
						<span class="info-user pass_width">确认密码：</span><input placeholder="请输入密码" type="password" class="china-name findjsBlank" rel="eqpassword" />
						<p class="info-tip hide"><span class="tenIcon err-icon"></span>请输入密码</p>
					</li>
				</ul>
				<div class="info-submit info-submit-findps">
				<button type="button" value="取消" class="infor-submit cancel-btn">取消</button>
				<input type="submit" value="确定" class="infor-submit" /></div>
			</form>
		</div>
	</div>
</div>
<!-- 公共尾部 -->
<% include ../common/b2s/b2s_footer.ejs %>
</body>
<% include ../common/bottom.ejs %>