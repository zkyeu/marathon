<% include ../common/top.ejs %>
<body class="ac-tem-choice ac-tem-tea ac-tem-read" data-init="acRead" swfHref="" audioPath="" page-tag="read,tea,result">
	<div class="ac-tem">
		<div class="ac-tem-tit">
			<span class="ac-tem-tit-bg"></span>
			<div class="ac-tem-tit-in ac-tem-wrap clearfix">
				<a href="javascript:void(0);" class="ac-close ac-pair-spirts fr">Go back to class</a>
			</div>
		</div>
	    <div class="ac-tem-con ac-tem-wrap">
	    	<div class="ac-tem-info">
				<ul class="clearfix">
	            	<li class="ac-timmer z2001" left-time="10">
	            		<span></span>
	            	</li>
	            	<!-- <li class="ac-beans">
                        <span>+999</span>
                    </li> -->
	            </ul>
	    	</div>
	        <div class="ac-tem-content">
                <!-- 排名 -->
                <div class="ac-tem-ranking">
                    <div class="ac-tem-ranking-bg"></div>
                    <p>题目</p>
                </div>
                <!-- 答题区域 -->
                <div class="choice-wrap">
                    <div class="choice-content">
                        <div class="read-content clearfix">
                            <div class="read-content-l fl more-read">
                                <span class="read-content-cover"></span>
                                <div class="read-content-img img-box">
                                    <ul id="teaImgList" class="clearfix">
                                        <li>
                                            <img src="<%= imagessrc%>ac-follow.jpg" />
                                        </li>
                                        <li>
                                            <img src="<%= imagessrc%>ac-demo.png" />
                                        </li>
                                        <li>
                                            <img src="<%= imagessrc%>ac-follow.jpg" />
                                        </li>
                                    </ul>
                                </div>
                                <div class="d-icons" id="tIndexImg">
                                    
                                </div>
                            </div>
                            <div class="tea-read-content-r fl">
                                <span class="play-btn-wrap" id="teaPlayBtn"><i class="play-btn"></i><em class="play-tip"></em></span>
                            </div>
                        </div>
                    </div>
                    <!-- <div class="ac-tem-tips">
                        <div class="ac-tem-tips-inner">
                            <ul class="clearfix">
                                <li>Speak slower, be encouraging and patientpatientpatient.</li>
                            </ul>
                        </div>
                    </div> -->
                    <i class="choice-grass"></i>
                </div>
            </div>
	    </div>
        <div class="ac-tem-result-mask"></div>
        <div class="ac-tem-result-wait">
            <div class="wait-tips">
                <a href="javascript:void(0);" class="wait-tips-close"></a>
                <dl class="wait-tips-in">
                    <dt>Student answering now</dt>
                    <dd><span>Tips：</span>please no speaking, the student is logining his/her voice, the student is logining his/her voice, the student is logining his/her voice</dd>
                </dl>
            </div>
        </div>
        <div class="ac-tem-result-con">
            <div class="ac-tem-result-con-in">
                <ul class="tea-choice-result">
                    <li>
                        <div class="read-result">
                            <p>Student's Answer：</p>
                            <p data-flag="choice-result" class="read-answer"></p>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
	</div>
<div class="choice-dialog choice-dialog-tea"></div>
<form name="class-info" action="http://www.baidu.com">
    <input type="hidden" name="q_id" value="100" />
    <input type="hidden" name="b2s_course_id" value="4653" />
</form>
<div id="player"></div>
<script type="text/javascript" src="http://sdk.cloud.chivox.com/chivoxsdk-js/v3.0/load_core.js"></script>
<script>
    var teaAudio = [
        {
            audioPath : 'http://sdk.cloud.chivox.com/chivoxsdk-js/v3.0/Examples/static/mp3/en/past.mp3'
        },
        {
            audioPath : '/images/html/ac_voice_share/horse.mp3'
        },
        {
            audioPath : 'http://sdk.cloud.chivox.com/chivoxsdk-js/v3.0/Examples/static/mp3/en/past.mp3'
        }
        ];
</script>
</body>
