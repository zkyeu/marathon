                                        <li>
                                            <img src="<%= imagessrc%>ac-follow.jpg" />
                                        </li>
                                    </ul>
                                </div>
                                <div class="d-icons" id="tIndexImg">
                                    
                                </div>
                            </div>
                            <div class="tea-read-content-r fl">
                                <span class="play-btn-wrap" id="teaPlayBtn"><i class="play-btn"></i><em class="play-tip"></em></span>
                            </div>
                        </div>
                    </div>
                    <div class="ac-tem-send">
                        <input type="button" class="ac-btn ac-send btn-disabled" disabled />
                        <a href="/templates/html/ac_dialog/ac_stu_read_demo.html" class="ac-tea-demo"></a>
                    </div>
