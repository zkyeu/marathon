                  <dt class="icon2"></dt>
                  <dd><input class="jsCheck" type="password" name="password" placeholder="填写密码" autocomplete="removepassword"></dd>
                </dl>
                <div class="u-err"></div>
              </li>
              <li>
                <dl>
                  <dt class="icon-user"></dt>
                  <dd><input class="jsEmpty jsCheck" type="text" name="user_name" placeholder="填写邮箱" autocomplete="removeemail"></dd>
