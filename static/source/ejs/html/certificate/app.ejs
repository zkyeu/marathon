						</div>
					</div>
					<!--证书介绍-->
					<%include ./module/introduce.ejs%>
					<!--获取方式介绍-->
					<%include ./module/access.ejs%>
				</div>
				<!--成人老学员即将获得证书 End-->
				<!--成人老学员获得证书列表 Start-->
				<div class="get switch">
					<!--证书列表-->
					<div class="cert-show">
						<%include ./module/list.ejs%>
						<%include ./module/list.ejs%>
						<%include ./module/list.ejs%>
					</div>
					<!--证书为空的占位图-->
					<%include ./module/placeholder.ejs%>
					<!--证书介绍-->
