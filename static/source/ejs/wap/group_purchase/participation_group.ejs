    </div>
    <div class="showtips-layer hide" id="showtipssLayer">
        <div class="showtips" id="showTips">
            <ul class="tab" id="regTabId">
                <li class="register">注册</li>
                <li class="t-flag login">登录</li>
            </ul>
            <div class="content" id="contentId">
                <div class="r-content">
                    <div class="input-b">
                        <input id="regPhone" type="text" placeholder="请输入手机号"/>
                    </div>
                    <div class="input-b">
                        <input id="regPassword" type="text" placeholder="设置您的密码"/>
                    </div>
                    <div class="input-b sms-b">
                        <input id="smsCode" class="sms-code" type="text" placeholder="获取短信验证码"/>
                        <div class="get-code" >
                            <a href="javascript:;" id="getCode">获取验证码</a>
                            <span class="send" id="countDown">重新发送(60)</span>
                        </div>
                    </div>
                    <p class="error" id="regError"></p>
                    <a class="btn" id="registerId" href="javascript:;">立即注册</a>
                </div>
                <div class="r-content hide">
                    <div class="input-b">
                        <input id="loginPhone" type="text" placeholder="请输入手机号"/>
                    </div>
                    <div class="input-b">
                        <input id="loginPassword" type="text" placeholder="请输入密码"/>
                    </div>
                    <p class="error" id="loginError"></p>
                    <a id="loginId" class="btn" href="javascript:;">立即登录</a>
                </div>
            </div>
        </div>
