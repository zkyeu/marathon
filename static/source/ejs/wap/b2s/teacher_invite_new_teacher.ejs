<body data-init="teaInviteNewTea">
	<div class="tea-invite-new-tea-wrap">
		<div class="form-wrap">
			<form action="#" autocomplete="off" rel="from">
				<div class="tea-invite-input">
						<input type="text" rel="teachername" name="X" class="input-t-style" placeholder="姓名" />
				</div>
				<p class="error-tips"></p>
				<div class="tea-invite-input">
						<input type="tel" rel="teacherphone" name="X" class="input-t-style" maxlength="11" placeholder="手机号" />
				</div>
				<p class="error-tips"></p>
				<div class="submit-btn">
					<input type="submit" class="input-submit" value="确定" />
				</div>
			</form>
		</div>
	</div>
</body>
