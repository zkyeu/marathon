	<section class="mask">
		<section class="share-guide">
			<section class="share-guide__arr"></section>
			<section class="share-guide__info">
				<img src="<%=imagessrc%>share_info.png">
			</section>
			<section class="share-guide__close"></section>
		</section>
		<section class="share-guide-reopen">
			<section class="share-guide-reopen__info">
				<img src="<%=imagessrc%>share_reopen.png">
